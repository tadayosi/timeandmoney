<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="example.doctorAppointments.AppointmentCalendar" table="APPOINTMENT_CALENDAR">
        <id name="id" column="ID">
            <generator class="increment"/>
        </id>
		<property name="defaultZoneID" column="DEFAULT_ZONE"/>
        <set name="events" table="APPOINTMENT_EVENT">
        	<key column="EVENT_ID"/>
        	<many-to-many column="APPOINTMENT_ID" class="example.doctorAppointments.Appointment"/>
    	</set>
    </class>
</hibernate-mapping>